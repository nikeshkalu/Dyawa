        "body-parser": "^1.19.0",
        "concurrently": "^5.3.0",
        "cross-env": "^7.0.2",
        "elliptic": "^6.5.4",
        "express": "^4.17.1",
        "framer-motion": "^2.9.4",
        "hex-to-binary": "^1.0.1",
        "history": "^5.0.0",
        "nodemon": "^2.0.4",
        "parcel-bundler": "^1.12.4",
        "pubnub": "^4.28.4",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-hover-animation": "^2.1.1",
